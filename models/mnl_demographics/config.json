{
  "model_info": {
    "name": "MNL Demographics",
    "description": "MNL with demographic interactions on fee and duration sensitivity",
    "parameters": ["ASC_paid", "B_FEE", "B_FEE_AGE", "B_FEE_EDU", "B_FEE_INC", "B_DUR", "B_DUR_EDU", "B_DUR_INC"],
    "true_values": {
      "ASC_paid": 5.0,
      "B_FEE": -0.08,
      "B_FEE_AGE": 0.06,
      "B_FEE_EDU": 0.08,
      "B_FEE_INC": 0.12,
      "B_DUR": -0.08,
      "B_DUR_EDU": -0.04,
      "B_DUR_INC": -0.03
    }
  },
  "population": {
    "N": 500,
    "T": 10,
    "seed": 42
  },
  "design": {
    "path": "../../data/raw/scenarios_prepared.csv",
    "sample_with_replacement": true
  },
  "demographics": {
    "age_idx": {
      "type": "categorical",
      "values": [0, 1, 2, 3, 4],
      "probs": [0.1, 0.35, 0.3, 0.2, 0.05],
      "center": 2.0,
      "scale": 2.0
    },
    "edu_idx": {
      "type": "categorical",
      "values": [0, 1, 2, 3, 4, 5],
      "probs": [0.02, 0.2, 0.15, 0.45, 0.15, 0.03],
      "center": 3.0,
      "scale": 2.0
    },
    "income_indiv_idx": {
      "type": "categorical",
      "values": [0, 1, 2, 3, 4, 5, 6, 7],
      "probs": [0.2, 0.2, 0.18, 0.14, 0.1, 0.08, 0.06, 0.04],
      "center": 3.0,
      "scale": 2.0
    }
  },
  "choice_model": {
    "fee_scale": 10000.0,
    "alternatives": {
      "1": {"name": "paid1", "asc": true},
      "2": {"name": "paid2", "asc": true},
      "3": {"name": "standard", "asc": false}
    },
    "base_terms": [
      {
        "name": "ASC_paid",
        "apply_to": ["paid1", "paid2"],
        "coef": 5.0
      }
    ],
    "attribute_terms": [
      {
        "name": "B_FEE",
        "attribute": "fee",
        "apply_to": ["paid1", "paid2"],
        "base_coef": -0.08,
        "interactions": [
          {"with": "age_idx", "coef": 0.06},
          {"with": "edu_idx", "coef": 0.08},
          {"with": "income_indiv_idx", "coef": 0.12}
        ]
      },
      {
        "name": "B_DUR",
        "attribute": "dur",
        "apply_to": ["paid1", "paid2", "standard"],
        "base_coef": -0.08,
        "interactions": [
          {"with": "edu_idx", "coef": -0.04},
          {"with": "income_indiv_idx", "coef": -0.03}
        ]
      }
    ]
  }
}
